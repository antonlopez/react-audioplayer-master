{
    "env": {
        "browser": true,
        "node": true
    },
    "ecmaFeatures": {
        "jsx": true
    },
    "globals": {
    },
    "plugins": [
    ],
    "extends": "eslint-config-airbnb",
    "parser": "babel-eslint",
    "rules": {
        "comma-dangle": 0,
        "no-console": 0,
        "id-length": 0,
        "react/prop-types": 0,
        "max-len": [
          2,
          {
            "code": 200,
            "tabWidth": 4,
            "ignoreUrls": true,
            "ignoreStrings": true
          }
        ],
        "react/jsx-boolean-value": 0,
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "class-methods-use-this": "off",
        "import/no-extraneous-dependencies": "warn"
    },
    "settings": {
        "import/extensions": [".js", ".jsx"]
    }
}
